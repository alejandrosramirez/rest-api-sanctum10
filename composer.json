{
    "name": "alejandrosramirez/rest-api-sanctum10",
    "type": "project",
    "description": "Rest API with Sanctum.",
    "keywords": [
        "framework",
        "laravel",
        "rest",
        "api",
        "php"
    ],
    "authors": [
		{
			"name": "Miguel Alejandro Salgado Ram√≠rez",
			"email": "alejandrosram@outlook.com"
		}
	],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "cerbero/eloquent-inspector": "^1.0",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "laravel/cashier": "^14.8",
        "laravel/framework": "^10.0",
        "laravel/octane": "^1.4",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.6",
        "laravel/telescope": "^4.13",
        "laravel/tinker": "^2.8",
        "laravel/vonage-notification-channel": "^3.2",
        "maatwebsite/excel": "^3.1",
        "maize-tech/laravel-markable": "^2.0",
        "matomo/device-detector": "^6.0",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-permission": "^5.9"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "knuckleswtf/scribe": "^4.15",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
			"@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
